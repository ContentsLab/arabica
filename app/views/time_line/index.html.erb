<% @urls.each do |url| %>
  <article>
    <header>
      <h1><%= url.title %></h1>
    </header>
    <p class='description'>
      <%= url.created_at %>
    </p>
    <footer>
      <p><%= url.url %></p>
      <ul>
      <% [].each do |tweet| %>
        <!-- <li><a href="<%= tweet.url %>"><img src="<%= tweet.user.icon %>" alt="<%= tweet.user %>" ></a></li> -->
      <% end %>
      </ul>
    </footer>
  </article>
<% end %>
